<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>1</portlet-name>
		<display-name>Events</display-name>
		<portlet-class>org.lsp.liferay.portlet.socialliving.events.portlet.EventsPortlet</portlet-class>
		<init-param>
			<name>clear-request-parameters</name>
			<value>true</value>
		</init-param>
		<init-param>
			<name>view-template</name>
			<value>/events/view.jsp</value>
		</init-param>
		<init-param>
            <name>config-template</name>
			<value>/events/configuration.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>Events</title>
			<short-title>Events</short-title>
			<keywords>Events</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>2</portlet-name>
		<display-name>Events Map</display-name>
		<portlet-class>org.lsp.liferay.portlet.socialliving.events.portlet.EventsMapPortlet</portlet-class>
		<init-param>
			<name>clear-request-parameters</name>
			<value>true</value>
		</init-param>
		<init-param>
			<name>view-template</name>
			<value>/events_map/view.jsp</value>
		</init-param>
		<init-param>
            <name>config-template</name>
			<value>/events_map/configuration.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>Events Map</title>
			<short-title>Events Map</short-title>
			<keywords>Events Map</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>