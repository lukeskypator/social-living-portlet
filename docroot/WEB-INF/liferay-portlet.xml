<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.1.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_1_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>1</portlet-name>
		<configuration-action-class>com.liferay.portal.kernel.portlet.DefaultConfigurationAction</configuration-action-class>
		<friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
		<friendly-url-mapping>events</friendly-url-mapping>
		<friendly-url-routes>org/lsp/liferay/portlet/socialliving/events/portlet/events-friendly-url-routes.xml</friendly-url-routes>
		<header-portlet-css>/events/css/main.css</header-portlet-css>
		<header-portlet-javascript>/events/js/events.js</header-portlet-javascript>
		<css-class-wrapper>social-living-portlet-events</css-class-wrapper>
		<add-default-resource>true</add-default-resource>
	</portlet>
	<portlet>
		<portlet-name>2</portlet-name>
		<configuration-action-class>com.liferay.portal.kernel.portlet.DefaultConfigurationAction</configuration-action-class>
		<private-request-attributes>true</private-request-attributes>
		<header-portlet-css>/events_map/css/events_map.css</header-portlet-css>
		<header-portlet-javascript>/events_map/js/events_map.js</header-portlet-javascript>
		<css-class-wrapper>social-living-portlet-events-map</css-class-wrapper>
		<add-default-resource>true</add-default-resource>
	</portlet>
	<portlet>
		<portlet-name>3</portlet-name>
		<icon>/common/icons/events-management.png</icon>
		<control-panel-entry-category>content</control-panel-entry-category>
		<control-panel-entry-weight>2.0</control-panel-entry-weight>
		<header-portlet-css>/events_management/css/events_management.css</header-portlet-css>
		<header-portlet-javascript>/events_management/js/events_management.js</header-portlet-javascript>
		<css-class-wrapper>social-living-portlet-events-management</css-class-wrapper>
		<add-default-resource>true</add-default-resource>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>